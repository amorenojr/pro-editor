{
    "comments": {
        "lineComment": "/*"
    },
    "brackets": [
        ["(",")"]
    ],
    "autoClosingPairs": [
        {"open":"{", "close":"}"},
        {"open":"[", "close":"]"},
        {"open":"(", "close":")"},
        {"open":"'", "close":"'", "notIn": ["string","comment"]},
        {"open":"\"", "close":"\"", "notIn": ["string"]},
        {"open":"`", "close":"`", "notIn": ["string","comment"]}
    ],
    "surroundingPairs": [
        ["{","}"],
        ["[","]"],
        ["(",")"],
        ["'","'"],
        ["\"","\""],
        ["`","`"]
    ],
    "folding": {
        "markers": {
            "start": "^\\/\\*\\s*#region\\b",
            "end": "^\\/\\*\\s*#endregion\\b"
        }
    }
}